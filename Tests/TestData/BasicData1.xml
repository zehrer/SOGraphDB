<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE gxl SYSTEM "http://www.gupro.de/GXL/gxl-1.0.dtd">
<!--
 The tag attr add a type-attribute which is a reference to a node id
 
 gxl use a type attribue with 
 -->
<gxl xmlns:xlink=" http://www.w3.org/1999/xlink">
    <graph id="1">
        <node id="1">
            <!-- type node -->
            <attr key="1">
                <string>label</string>
            </attr>
        </node>
        <node id="2">
            <attr key="1">
                <string>A</string>
            </attr>
        </node>
        <node id="3">
            <attr key="1">
                <string>B</string>
            </attr>
        </node>
        <node ID="4">
            <!-- error case ID in low case-->
            <attr key="1">
                <string>C</string>
            </attr>
        </node>
        <edge id="1" to="1" from="2">
            <attr key="1">
                <int>42</int>
            </attr>
        </edge>
    </graph>
</gxl>
